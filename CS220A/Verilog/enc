L_0x7f301583d018 .functor BUFT 1, C4<0>, C4<0>, C4<0>, C4<0>;
v0x561bfb3d9df0_0 .net "y", 0 0, L_0x7f301583d018;  1 drivers
E_0x561bfb3c81c0 .event edge, v0x561bfb38d030_0;
S_0x561bfb3d9ef0 .scope module, "obj" "encode" 2 6, 4 1 0, S_0x561bfb3c4f60;
 .timescale 0 0;
    .port_info 0 /INPUT 8 "in"
    .port_info 1 /OUTPUT 3 "out"
v0x561bfb3da100_0 .net "in", 7 0, v0x561bfb3d9d10_0;  alias, 1 drivers
v0x561bfb3da210_0 .var "out", 2 0;
E_0x561bfb3a20f0 .event edge, v0x561bfb3d9d10_0;
    .scope S_0x561bfb3c50e0;
T_0 ;
    %wait E_0x561bfb3c81c0;
    %pushi/vec4 0, 0, 8;
    %store/vec4 v0x561bfb3d9d10_0, 0, 8;
    %pushi/vec4 1, 0, 1;
    %ix/getv 4, v0x561bfb38d030_0;
    %store/vec4 v0x561bfb3d9d10_0, 4, 1;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x561bfb3d9ef0;
T_1 ;
    %wait E_0x561bfb3a20f0;
    %load/vec4 v0x561bfb3da100_0;
    %parti/s 4, 4, 4;
    %or/r;
    %ix/load 4, 2, 0;
    %flag_set/imm 4, 0;
    %store/vec4 v0x561bfb3da210_0, 4, 1;
    %load/vec4 v0x561bfb3da100_0;
    %parti/s 1, 2, 3;
    %load/vec4 v0x561bfb3da100_0;
    %parti/s 1, 3, 3;
    %or;
    %load/vec4 v0x561bfb3da100_0;
    %parti/s 1, 6, 4;
    %or;
    %load/vec4 v0x561bfb3da100_0;
    %parti/s 1, 7, 4;
    %or;
    %ix/load 4, 1, 0;
    %flag_set/imm 4, 0;
    %store/vec4 v0x561bfb3da210_0, 4, 1;
    %load/vec4 v0x561bfb3da100_0;
    %parti/s 1, 1, 2;
    %load/vec4 v0x561bfb3da100_0;
    %parti/s 1, 3, 3;
    %or;
    %load/vec4 v0x561bfb3da100_0;
    %parti/s 1, 5, 4;
    %or;
    %load/vec4 v0x561bfb3da100_0;
    %parti/s 1, 7, 4;
    %or;
    %ix/load 4, 0, 0;
    %flag_set/imm 4, 0;
    %store/vec4 v0x561bfb3da210_0, 4, 1;
    %jmp T_1;
    .thread T_1, $push;
    .scope S_0x561bfb3c4f60;
T_2 ;
    %wait E_0x561bfb3c5970;
    %vpi_call 2 8 "$display", "%d %b %b %b", $time, v0x561bfb3da330_0, v0x561bfb3da4f0_0, v0x561bfb3da420_0 {0 0 0};
    %jmp T_2;
    .thread T_2, $push;
    .scope S_0x561bfb3c4f60;
T_3 ;
    %pushi/vec4 0, 0, 3;
    %store/vec4 v0x561bfb3da330_0, 0, 3;
    %delay 1, 0;
    %pushi/vec4 1, 0, 3;
    %store/vec4 v0x561bfb3da330_0, 0, 3;
    %delay 1, 0;
    %pushi/vec4 2, 0, 3;
    %store/vec4 v0x561bfb3da330_0, 0, 3;
    %delay 1, 0;
    %pushi/vec4 3, 0, 3;
    %store/vec4 v0x561bfb3da330_0, 0, 3;
    %delay 1, 0;
    %pushi/vec4 4, 0, 3;
    %store/vec4 v0x561bfb3da330_0, 0, 3;
    %delay 1, 0;
    %pushi/vec4 5, 0, 3;
    %store/vec4 v0x561bfb3da330_0, 0, 3;
    %delay 1, 0;
    %pushi/vec4 6, 0, 3;
    %store/vec4 v0x561bfb3da330_0, 0, 3;
    %delay 1, 0;
    %pushi/vec4 7, 0, 3;
    %store/vec4 v0x561bfb3da330_0, 0, 3;
    %end;
    .thread T_3;
    .scope S_0x561bfb3c4f60;
T_4 ;
    %delay 20, 0;
    %vpi_call 2 29 "$finish" {0 0 0};
