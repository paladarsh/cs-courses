S_0x561bfb3c4f60 .scope module, "top" "top" 2 1;
 .timescale 0 0;
v0x561bfb3da330_0 .var "in", 2 0;
v0x561bfb3da420_0 .net "out", 2 0, v0x561bfb3da210_0;  1 drivers
v0x561bfb3da4f0_0 .net "z", 7 0, v0x561bfb3d9d10_0;  1 drivers
E_0x561bfb3c5970 .event edge, v0x561bfb3da210_0;
S_0x561bfb3c50e0 .scope module, "ob" "decode" 2 5, 3 1 0, S_0x561bfb3c4f60;
 .timescale 0 0;
    .port_info 0 /INPUT 3 "in"
    .port_info 1 /OUTPUT 8 "out"
v0x561bfb38d030_0 .net "in", 2 0, v0x561bfb3da330_0;  1 drivers
